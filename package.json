{
  "name": "opensink",
  "version": "0.1.1",
  "description": "Official JavaScript/TypeScript client for the OpenSink API",
  "private": false,
  "type": "module",
  "license": "MIT",
  "author": "OpenSink <hello@opensink.com> (https://opensink.com)",
  "homepage": "https://github.com/opensinkai/opensink-javascript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensinkai/opensink-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/opensinkai/opensink-javascript/issues"
  },
  "keywords": [
    "opensink",
    "ai",
    "agents",
    "api",
    "sdk"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint:fix": "yarn lint --fix",
    "lint": "eslint ./src --ext .ts",
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@stylistic/eslint-plugin": "5.2.2",
    "@types/node": "24.1.0",
    "eslint": "9.32.0",
    "globals": "16.3.0",
    "typescript": "5.8.3",
    "tsc-alias": "1.8.16",
    "typescript-eslint": "8.38.0"
  },
  "dependencies": {
    "axios": "^1.13.4"
  }
}
